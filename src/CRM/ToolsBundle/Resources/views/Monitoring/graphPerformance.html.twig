    <div class="container containerPage">
        <h1 class="titleGraph">Graphs Performance</h1>
        <!-- Begin of datepicker form -->
        <div class="bootstrap-iso">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <!-- Form code begins -->
